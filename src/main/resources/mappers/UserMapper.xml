<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.csii.lipei.springbootProject.mapper.UserMapper" >
	<!-- 开启二级缓存 -->
	<cache 
	eviction="FIFO" flushInterval="60000" size="512" readOnly="true"/> 
	<resultMap type="com.csii.lipei.springbootProject.entity.User" id="queryUserrResult">
		<id property="userId" column="id"/>
		<result property="name" column="name"/>
		<result property="age" column="age"/>
	</resultMap>
	<select id="queryUser" resultMap="queryUserrResult" >
		select * from lipeiUser
	</select>
	
</mapper>